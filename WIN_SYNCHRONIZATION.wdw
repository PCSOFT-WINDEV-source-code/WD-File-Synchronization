   -
     name : ProjectUPD
     procedure_id : 1760085612327950203
     type_code : 14
     code : |1-
      // Summary: Analyze and synchronize the synchronization project
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ProjectUPD()
      
      // Local variables
      arrExceptionDirectory is array of string
      arrExceptionExtensions is array of string
      
      gbDeletion = PROJECT.Copyld
      
      // Read the exceptions
      StringToArray(PROJECT.ExceptionDirectories,arrExceptionDirectory,CR)
      StringToArray(PROJECT.Exceptionfiles,arrExceptionExtensions,CR)
      
      // initialize the controls
      EDT_SOURCE_DIRECTORY  = CompleteDir(PROJECT.SourceDir)
      EDT_DESTINATION_DIRECTORY = CompleteDir(PROJECT.DestDir)
      
      // Configure the exceptions
      COL_Synchronization.DefineException.arrListExceptionDirectories =arrExceptionDirectory
      COL_Synchronization.DefineException.arrListExceptionExtensions =arrExceptionExtensions
      COL_Synchronization.DefineException()
      
      // calculate the synchronization of two directories	
      COL_Synchronization.AnalyzeDirectories.sSourceDirectoryName = CompleteDir(PROJECT.SourceDir)
      COL_Synchronization.AnalyzeDirectories.sNameDestinationDirectory = CompleteDir(PROJECT.DestDir)
      COL_Synchronization.AnalyzeDirectories.bAnalyzeSubDirectory = PROJECT.SubDir
      COL_Synchronization.AnalyzeDirectories.bOverwriteMoreRecentFiles = PROJECT.ReverseUPD
      IF	COL_Synchronization.AnalyzeDirectories() = False THEN
      	Error("The comparison cannot be performed",ErrorInfo())
      	RETURN	
      END
      
      // Fill the comparison table
      FillComparisonTable()
      
      // copy of files
      Update()
     type : 458752
   -
     name : SynchronizationStatusReport
     procedure_id : 1760085612328015739
     type_code : 14
     code : |1+
      // Summary: Displays the status report of synchronization
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE SynchronizationStatusReport()
      
      // Local variables
      sMessage is string
      
      SWITCH gnFileCopy
      	CASE 0,1
      		sMessage = StringBuild(<§$0003§>,gnFileCopy,gnFiletoCopy)
      	OTHER CASE
      		sMessage = StringBuild(<§$0004§>,gnFileCopy,gnFiletoCopy)
      END
      
      // identical copy mode
      IF gbDeletion THEN
      	IF gnFiletoDelete>0 THEN
      		SWITCH gnFileDelete
      			CASE 0,1
      				sMessage+= StringBuild(<§$0005§>, gnFileDelete, gnFiletoDelete)
      			OTHER CASE
      				sMessage+= StringBuild(<§$0006§>, gnFileDelete, gnFiletoDelete)
      		END
      	END
      END
      
      // Displaying the message	
      Message(sMessage)
     type : 458752
   -
     name : Update
     procedure_id : 1760085612328081275
     type_code : 14
     code : |1-
      // Summary: Effective request for synchronization
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE Update()
      
      // Local variables
      nSubscript is int
      nLines is int 
      
      // Initialize the number of files to process
      nLines = TableCount(ListTable)
      gnFileCopy = 0
      gnFileDelete = 0
      
      HourGlass(True)
      
      // browse the table of files to process
      FOR nSubscript=1 _TO_ nLines
      	// display the progress bar
      	Gauge(nSubscript,nLines)
      	
      	// if the check box is checked
      	IF COL_ENABLED[nSubscript] = True THEN
      
      		// if it's a file to copy
      		IF COL_REPORT[nSubscript] = CST_FILE_TO_COPY THEN
      			
      			// copy the file
      			LOOP
      				IF COL_Synchronization.ApplyFileSynchronization(SET_FILE[nSubscript]) = True THEN					
      					gnFileCopy++
      					COL_REPORT[nSubscript] = CST_FILE_COPY
      					BREAK
      				END
      				
      				IF YesNo(StringBuild(<§$0009§>, SET_FILE[nSubscript]), <§$000a§>, <§$000b§>)=No THEN 
      					COL_REPORT[nSubscript] = CST_FILE_COPY_FAILURE
      					BREAK
      				END	
      			END
      		END	
      			
      		// if it's a file to delete	
      		IF COL_REPORT[nSubscript] = CST_FILE_TO_DELETE  THEN
      			LOOP
      				// delete the file
      				IF COL_Synchronization.ApplyFileSynchronization(SET_FILE[nSubscript]) = True THEN		
      					gnFileDelete++
      					COL_REPORT[nSubscript] = CST_FILE_DELETE
      					BREAK
      				END
      				
      				IF YesNo(StringBuild(<§$000f§>,SET_FILE[nSubscript]), <§$000a§>, <§$000b§>)=No THEN 
      					COL_REPORT[nSubscript] = CST_FILE_DELETE_FAILURE
      					BREAK
      				END	
      			END
      		END
      	
      	// if the check box is not checked	
      	ELSE
      		COL_REPORT[nSubscript] = CST_FILE_IGNORE
      	END
      	
      	WinRedraw()
      END
      
      // status report
      SynchronizationStatusReport()
      
      HourGlass(False)
     type : 458752
   -
     name : ListingReview
     procedure_id : 1760085612328146811
     type_code : 14
     code : |1+
      // Summary: Displays the status report of synchronization
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ListingReview()
      
      // if no file to process
      IF (gnFiletoCopy=0 AND gnFiletoDelete=0) THEN
      	Message(<§$0018§>)
      	IF gbAuto = False THEN
      		Info(<§$0018§>)
      	END
      	
      ELSE
      	sMessage is string
      	// message to display
      	SWITCH gnFiletoCopy
      		CASE 0
      			sMessage = <§$0019§>
      		CASE 1
      			sMessage = <§$001a§>
      		OTHER CASE
      			sMessage = StringBuild(<§$001b§>,gnFiletoCopy)
      	END
      	
      	// case of identical copy
      	IF gbDeletion THEN
      		SWITCH gnFiletoDelete
      			CASE 0
      				sMessage+=""
      			CASE 1	
      				sMessage+=<§$001c§>
      			OTHER CASE
      				sMessage+= StringBuild(<§$001d§>, gnFiletoDelete)
      		END
      	END
      	
      	// display the message	
      	Message(sMessage)
      
      END
