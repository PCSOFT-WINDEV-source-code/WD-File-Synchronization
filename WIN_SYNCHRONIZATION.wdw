      // Summary: Loads the configuration of synchronization project
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE LoadProject()
      
      gbSubDir = PROJECT.SubDir
      gbCrash = PROJECT.ReverseUPD
      gbDeletion = PROJECT.Copyld
      
      // initialize the controls
      EDT_SOURCE_DIRECTORY  = CompleteDir(PROJECT.SourceDir)
      EDT_DESTINATION_DIRECTORY = CompleteDir(PROJECT.DestDir)
     type : 458752
   -
     name : DisplayComparison
     procedure_id : 1760085612328277883
     type_code : 14
     code : |1+
      // Summary: Displays the result of synchronization
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DisplayComparison()
      
      // Local variables
      sCurrentFile		is string
      
      // Delete the previous content
      TableDeleteAll(ListTable)
      
      // Browse the files to copy
      FOR EACH sCurrentFile of COL_Synchronization.garrDestinationFileToCopy
      	TableAddLine(ListTable,True,sCurrentFile,CST_FILE_TO_COPY)
      END
      
      // Browse the files to delete
      IF gbDeletion = True THEN
      	FOR EACH sCurrentFile of COL_Synchronization.garrDestinationFileToDelete
      		TableAddLine(ListTable,True,sCurrentFile,CST_FILE_TO_DELETE)
      	END
      	
      END
      
      // Store the number of files to copy and to delete
      gnFiletoCopy = COL_Synchronization.garrDestinationFileToCopy..Occurrence
      gnFiletoDelete = COL_Synchronization.garrDestinationFileToDelete..Occurrence
     type : 458752
   -
     name : FillComparisonTable
     procedure_id : 1760085612328343419
     type_code : 14
     code : |1+
      // Summary: Fills the table with the synchronization analysis
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE FillComparisonTable()
      
      // Local variables
      arrExceptionDirectory is array of string
      arrExceptionExtensions is array of string
      
      IF ListCount(PROJECT_Combo)<=0 THEN RETURN
      
      // Start filling the table
      TableStartFilling(ListTable)
      
      // Read the exceptions
      IF PROJECT.ExceptionDirectories<>"" THEN
      	StringToArray(PROJECT.ExceptionDirectories,arrExceptionDirectory,CR)
      END
      
      IF PROJECT.Exceptionfiles<>"" THEN
      	StringToArray(PROJECT.Exceptionfiles,arrExceptionExtensions,CR)
      END
      
      // Configure the exceptions
      COL_Synchronization.DefineException.arrListExceptionDirectories =arrExceptionDirectory
      COL_Synchronization.DefineException.arrListExceptionExtensions =arrExceptionExtensions
      COL_Synchronization.DefineException()
      
      // calculate the synchronization of two directories	
      COL_Synchronization.AnalyzeDirectories.sSourceDirectoryName = EDT_SOURCE_DIRECTORY
      COL_Synchronization.AnalyzeDirectories.sNameDestinationDirectory = EDT_DESTINATION_DIRECTORY
      COL_Synchronization.AnalyzeDirectories.bAnalyzeSubDirectory = gbSubDir
      COL_Synchronization.AnalyzeDirectories.bOverwriteMoreRecentFiles = gbCrash
      IF COL_Synchronization.AnalyzeDirectories() = False THEN
      	Error("The comparison cannot be performed",ErrorInfo())
      END
      
      // Display the result of proposed synchronization
      DisplayComparison()
      
      // Stop filling the table
      TableEndFilling(ListTable)
      
