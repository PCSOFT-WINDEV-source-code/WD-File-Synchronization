      CONSTANT
      	CST_FILE_TO_COPY = <§$0014§>
      	CST_FILE_TO_DELETE = <§$0016§>
      	CST_FILE_COPY = <§$0008§>
      	CST_FILE_COPY_FAILURE = <§$000c§>
      	CST_FILE_DELETE = <§$000e§>
      	CST_FILE_DELETE_FAILURE = <§$0010§>
      	CST_FILE_IGNORE = <§$0011§>
      END
      
      GLOBAL
      	gnFiletoCopy, gnFiletoDelete are int // number of files to copy and to delete
      	gnFileCopy, gnFileDelete are int // number of copied files that must be deleted
   -
     code : |1+
      // if there is no project, open the configuration window
      HReadFirst(PROJECT,Name)
      IF HOut(PROJECT) THEN
      	Open(WIN_PROJECT)
      END
      
      // if there is a command line
      IF gsCommandLine<>"" THEN
      	
      	// We are in automatic mode
      	gbAuto = True
      	DelayBeforeClosing(500)
      	OpenChild(WIN_AUTOMATIC_PROCESS)
      	
      	IF gsCommandLine<>"*" THEN
      		// find the project
      		HReadSeekFirst(PROJECT,Name,gsCommandLine)
      		IF HFound() THEN
      			// title of the window
      			WIN_AUTOMATIC_PROCESS..Title = <§$0000§>+PROJECT.Name
      			WIN_AUTOMATIC_PROCESS.Stc_Source_Dir = <§$0001§>+PROJECT.SourceDir
      			WIN_AUTOMATIC_PROCESS.Stc_Dest_Dir = <§$0002§>+PROJECT.DestDir
      		
      			ProjectUPD()
      		END
      
      	// all projects	
      	ELSE
      		HReadFirst(PROJECT,Name)
      		WHILE NOT HOut(PROJECT)
      			// title of the window
      			WIN_AUTOMATIC_PROCESS..Title = <§$0000§>+PROJECT.Name
      			WIN_AUTOMATIC_PROCESS.Stc_Source_Dir = <§$0001§>+PROJECT.SourceDir
      			WIN_AUTOMATIC_PROCESS.Stc_Dest_Dir = <§$0002§>+PROJECT.DestDir
      			ProjectUPD()
      			HReadNext(PROJECT,Name)
      		END
      	END
      
      	EndProgram()
      
      ELSE
      	// in visual mode
      	gbAuto = False
      	
      	// Is a project defined?
      	IF ListCount(PROJECT_Combo)>0 THEN
      		// first project of the combo box
      		ListSelectPlus(PROJECT_Combo,1)
      		HReadSeekFirst(PROJECT,PROJECTID,PROJECT_Combo)
      		LoadProject()
      	END	
      END
